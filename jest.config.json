{
  "testEnvironment": "jsdom",
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/platforms/",
    "/plugins/",
    "/lib/",
    "/manual_lib/"
  ],
  "transform": {
    "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!(@react-native|react-native|react-native-vector-icons))"
  ],
  "moduleNameMapper": {
    "^react-native$": "react-native-web"
  }
}
